{
  "singleQuote": true,
  "printWidth": 100,
  "trailingComma": "all",
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": ["*.json", ".prettierrc"],
      "options": { "parser": "json" }
    },
    {
      "files": ["*.{yaml,yml}"],
      "options": { "singleQuote": false }
    }
  ]
}
